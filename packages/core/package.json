{
  "name": "@connect2ic/core",
  "author": "Mio Quispe",
  "private": false,
  "version": "0.0.4",
  "types": "./dist/declarations/src/index.d.ts",
  "files": [
    "dist",
    "providers",
    "style.css"
  ],
  "main": "dist/connect2ic-core.cjs.js",
  "module": "dist/connect2ic-core.esm.js",
  "exports": {
    "./package.json": "./package.json",
    "./style.css": "./src/style.css",
    "./providers": {
      "module": "./providers/dist/connect2ic-core-providers.cjs.js",
      "default": "./providers/dist/connect2ic-core-providers.esm.js"
    },
    ".": {
      "import": "./dist/connect2ic-core.esm.js",
      "browser": "./dist/connect2ic-core.esm.js",
      "module": "./dist/connect2ic-core.esm.js",
      "default": "./dist/connect2ic-core.cjs.js",
      "require": "./dist/connect2ic-core.cjs.js"
    }
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts"
    ]
  },
  "sideEffects": false,
  "dependencies": {
    "@astrox/connection": "^0.0.40",
    "@dfinity/agent": "0.10.4",
    "@dfinity/auth-client": "0.10.4",
    "@dfinity/authentication": "0.10.4",
    "@dfinity/candid": "0.10.4",
    "@dfinity/identity": "0.10.4",
    "@dfinity/principal": "0.10.4",
    "ic-stoic-identity": "^2.0.0",
    "secp256k1": "^4.0.3",
    "xstate": "^4.30.5"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "buffer": "^6.0.3",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "turbo": "^1.1.9",
    "typescript": "^4.4.3",
    "vite": "^2.5.7"
  }
}
